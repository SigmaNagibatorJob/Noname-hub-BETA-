local Window = OrionLib:MakeWindow({Name = "AutoFarm Script", HidePremium = true, SaveConfig = true, ConfigFolder = "Orion"})

local MainTab = Window:MakeTab({
    Name = "Главное",
    Icon = "rbxassetid://1234567890",
    PremiumOnly = false
})

-- Вкладка Персонаж
local CharacterTab = Window:MakeTab({
    Name = "Персонаж",
    Icon = "rbxassetid://1234567890",
    PremiumOnly = false
})

-- Переменная для скорости игры
local speedValue = _G.Speed or 23

-- Добавление слайдера для изменения скорости игры
CharacterTab:AddSlider({
    Name = "Скорость игры",
    Min = 10,
    Max = 100,
    Default = speedValue,
    Color = Color3.fromRGB(0, 128, 255),
    Increment = 1,
    Callback = function(value)
        speedValue = value
        game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = speedValue
        OrionLib:MakeNotification({
            Name = "Скорость игры",
            Content = "Скорость игры изменена на: " .. speedValue,
            Image = "rbxassetid://1234567890",
            Time = 5
        })
    end
})

-- Остальной код из вашего интерфейса...

OrionLib:Init()
